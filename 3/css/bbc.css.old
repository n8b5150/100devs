*{
  font-family: 'Calibri', 'Helvetica', 'Arial', sans-serif;
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

.clear{
  clear: both;
}

header{
  height: 60px;
  background: black;
  padding: 0 25px;
  margin: 0;
}
header *{
  color: white;
  background: black;
  display: inline;
}
header > section{
  padding-top: 15px;
}

.logo{
  border: 1px solid black;
  background: white;
  color: black;
  text-transform: uppercase;
  font-size: 2rem;
  padding: 0 3px;
}
ul{
  list-style: none;
}
li{
  padding: 20px 10px;
  margin: 0;
  border: 1px solid white;
  border-right: 0;
  border-top: 0;
  border-bottom: 0;
  display: inline-block;
}
a{
  text-decoration: none;
}
input{
  background: white;
  color: black;
}
.title{
  text-align: left;
}
.date{
  text-align: right;
}
.invert{
  color: white;
}
h1,h2,p{
  padding-bottom: 7px;
}
h1{
  font-size: 2.5rem;
  font-weight: normal;
}
h2{
  font-size: 1.25rem;
}
.headlinenormal{
  font-weight: normal;
}
.leftborder{
  border: 2px solid red;
  border-right: 0;
  border-top: 0;
  border-bottom: 0;
  padding-left: 5px;
}
.borderblue{
  border-color: blue;
}
.bordergray{
  border-color: gray;
}
.caps{
  text-transform: uppercase;
}
.mainheadline{
  background-image: url("../assets/israel.jpg");
  background-size: contain;
  background-repeat: no-repeat;
  width: 50%;
  height:400px;
  border:5px solid white;
}


.quarters{
  width:50%;
  height:400px;
}
.q1,.q2,.q3,.q4{
  width: 50%;
  height: 190px;
  padding: 10px;
  border:5px solid white;
  background-size:contain;
  background-repeat: no-repeat;
}
.q1{
  background-image: url("../assets/gohome.jpg");
}
.q2{
  background-image: url("../assets/vaccine.jpg");
}
.q3{
  background-image: url("../assets/workgyms.webp");
}
.q4{
  background-image: url("../assets/folksinger.webp");
}

.thirds{
  width: 33%;
}
.thirds img{
  display: block;
  max-width: 425px;
  padding: 5px;
  border-radius: 20px;
  border: 2px solid white;
}

.welcomebar,main{
  width: 100%;
  float: left;
}
.welcome,.mainheadline,.quarters,.q1,.q2,.q3,.q4,.thirds{
  float: left;
}
